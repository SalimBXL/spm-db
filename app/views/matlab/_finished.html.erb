
<div class="container">

    <br>

    <div class="row">
        <div class="col">
            <div class="card">
                <div class="card-body">
                    <dl>
                        <dt>CAMERA</dt>
                        <dd>Getting camera... <span class="text-success material-icons" id="checked_camera">done</span></dd>
                    </dl>
                </div>
            </div>
        </div>
        <div class="col"></div>
    </div>

    <div class="row">
        <div class="col">
            <div class="card">
                <div class="card-body">
                    <dl>
                        <dt>DICOM</dt>
                        <dd>Getting images... <span class="text-success material-icons" id="checked_zip">done</span></dd>
                        <dd>Reading header... <span class="text-success material-icons" id="checked_read">done</span></dd>
                    </dl>
                </div>
            </div>
        </div>
        <div class="col">
            <div class="d-flex justify-content-center">
                <div class="p-2" id="button_next">
                    <a href="<%= root_path %>" 
                        class="btn btn-success"
                        role="button">
                        <span class="material-icons">check_circle_outline</span><br>
                        Finish
                    </a>
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col">
            <div class="card">
                <div class="card-body">
                    <dl>
                        <dt>MATLAB</dt>
                        <dd>Starting Matlab... <span class="text-success material-icons" id="checked_matlab">done</span></dd>
                        <dd>Adding PDF's to database... <span class="text-success material-icons" id="checked_pdf">done</span></dd>
                    </dl>
                </div>
            </div>
        </div>
        <div class="col">
        </div>
    </div>

    <div class="row">
        <div class="col">
            <div class="card">
                <div class="card-body">
                    <dl>
                        <dt>MISCELLANEOUS</dt>
                        <dd>Removing DICOM entry from server... <span class="text-success material-icons" id="checked_remove">done</span></dd>
                    </dl>
                </div>
            </div>
        </div>
        <div class="col">
        </div>
    </div>
